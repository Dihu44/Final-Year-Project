				&lt;/tr&gt;&lt;tr&gt;&lt;td class="text-align-right"&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;strong&gt;&lt;span lang="EN-ZA" xml:lang="EN-ZA" xml:lang="EN-ZA"&gt;&lt;span&gt;Branch code:&lt;/span&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
					&lt;td&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang="EN-ZA" xml:lang="EN-ZA" xml:lang="EN-ZA"&gt;&lt;span&gt;662167&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
				&lt;/tr&gt;&lt;tr&gt;&lt;td class="text-align-right"&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;strong&gt;&lt;span lang="EN-ZA" xml:lang="EN-ZA" xml:lang="EN-ZA"&gt;&lt;span&gt;Swift code:&lt;/span&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
					&lt;td&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span&gt;&lt;span lang="EN-ZA" xml:lang="EN-ZA" xml:lang="EN-ZA"&gt;&lt;span&gt;SCHBBWGX&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/td&gt;
